decl
    integer arr[10];
    integer target;
    integer low;
    integer high;
    integer mid;
    integer found;
enddecl

begin
    arr[0] = 3;
    arr[1] = 7;
    arr[2] = 11;
    arr[3] = 15;
    arr[4] = 19;
    arr[5] = 23;
    arr[6] = 27;
    arr[7] = 31;
    arr[8] = 35;
    arr[9] = 39;

    target = 27;
    found = -1;
    low = 0;
    high = 9;
    
    
    for (; low <= high;) {
        mid = (low + high) / 2;
        if arr[mid] == target then
            found = mid;
            break;
        else
            if arr[mid] < target then
                low = mid + 1;
            else
                high = mid - 1;
            endif
        endif
    }
    
    if found != -1 then
        write("Element found at index", found);
    else
        write("Element not found");
    endif
    
end
