decl
    integer arr[10];
    integer i;
    integer j;
    integer temp;
enddecl

begin
    arr[0] = 34;
    arr[1] = 7;
    arr[2] = 23;
    arr[3] = 32;
    arr[4] = 5;
    arr[5] = 62;
    arr[6] = 12;
    arr[7] = 9;
    arr[8] = 1;
    arr[9] = 45;
    
    for ( i = 0; i < 10; i = i + 1 ) {
        for ( j = 0; j < 9 - i; j = j + 1 ) {
            if arr[j] > arr[j+1] then
                temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
            endif
        }
    }
    
    write("Sorted array:");
    for ( i = 0; i < 10; i = i + 1 ) {
        write(arr[i]);
    }
    
end
